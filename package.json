{
  "displayName": "BAChome",
  "name": "homebridge-bachome",
  "version": "1.4.1",
  "description": "A short description about what your plugin does.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/caserage/bachome.git"
  },
  "bugs": {
    "url": "https://github.com/caserage/bachome/issues"
  },
  "engines": {
    "node": ">=16.10.0",
    "homebridge": ">=1.3.4"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ignore-path .eslintignore \"src/**/*.+(js|ts|json)\"",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "dependencies": {
    "bacstack": "0.0.1-beta.13"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "homebridge": "^1.3.4",
    "homebridge-config-ui-x": "^4.6.7",
    "nodemon": "^2.0.13",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.4.3"
  }
}
